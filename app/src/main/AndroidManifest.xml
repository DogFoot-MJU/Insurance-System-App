<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dogfoot.insurancesystemapp">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/dogfoot_icon"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InsuranceSystemApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".isApp.jungwoo.domain.legitimateexamination.view.LegitimateExaminationFirstFragment"
            tools:ignore="Instantiatable" />
        <activity android:name=".isApp.jungwoo.JungWoo" />
        <activity android:name=".isApp.jungwoo.intro.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".isApp.dongwook.domain.logout.view.LogOutActivity" />
        <activity android:name=".isApp.dongwook.domain.customerConsulting.view.CustomerConsultingMainActivity" />
        <activity android:name=".isApp.dongwook.domain.salesConsulting.view.SalesConsultingMainActivity" />
        <activity android:name=".isApp.dongwook.DongWookActivity" />
        <activity android:name=".isApp.dongwook.domain.login.view.LoginActivity" />
        <activity android:name=".isApp.dongwook.domain.signUp.view.SignUpActivity" />
        <activity android:name=".isApp.dongwook.domain.customerCompensation.view.CustomerCompensationUploadActivity"/>
        <activity android:name=".isApp.dongwook.domain.customerCompensation.view.CustomerCompensationMainActivity"/>
        <activity android:name=".isApp.dongwook.domain.salesCompensation.view.SalesCompensationMainActivity"/>
        <activity android:name=".isApp.dongwook.domain.salesCompensation.view.SalesCompensationViewActivity"/>
        <activity android:name=".isApp.MainActivity" />
    </application>

</manifest>
