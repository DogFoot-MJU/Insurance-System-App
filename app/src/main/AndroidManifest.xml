<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dogfoot.insurancesystemapp">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:usesCleartextTraffic="true"
        android:hardwareAccelerated="true"
        android:theme="@style/Theme.InsuranceSystemApp">
        <activity android:name=".isApp.jungwoo.JungWoo"/>
        <activity android:name=".isApp.dongwook.DongWookActivity" />
        <activity android:name=".isApp.dongwook.domain.login.view.LoginActivity"/>
        <activity android:name=".isApp.dongwook.domain.signUp.view.SignUpActivity"/>
        <activity android:name=".isApp.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>